            </td>

            <td><input type="number" readonly id="tuikTotal" text="0" /></td>
            <td><input type="number" readonly id="tmoTotal" text="0" /></td>
            <td><input type="number" readonly id="percentTotal" text="0" /></td>
            <td><input type="number" readonly id="hasatedilenTotal" text="0" /></td>
            <td><input type="number" readonly id="bekleyentonTotal" text="0" /></td>

            @for (int i = 0; i < 5; i++)
            {
                <td><input type="number" readonly id="@("toplanaturel_"+i.ToString())" text="0" /></td>
            }

            <td><input type="number" readonly id="piyasatonTotal" text="0" /></td>

            @for (int i = 0; i < 5; i++)
            {
                <td><input type="number" readonly text="0" id="@("toptan"+i)" /></td>
            }


            <td><input style="border:none" readonly id="dfiyatTotal" text="0" class="inputtop" /></td>
            <td><input style="border:none" readonly id="yfiyatTotal" text="0" class="inputtop" /></td>
            <td><input style="border:none" readonly id="ofiyatTotal" text="0" /></td>

            <td><input style="border:none" readonly id="dfiyat2Total" text="0" class="inputtop" /></td>
            <td><input style="border:none" readonly id="yfiyat2Total" text="0" class="inputtop" /></td>
            <td><input style="border:none" readonly id="ofiyat2Total" text="0" /></td>

        </tr>

    </tbody>
</table>
