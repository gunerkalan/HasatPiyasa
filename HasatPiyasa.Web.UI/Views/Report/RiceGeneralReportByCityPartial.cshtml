        </tr>
    </thead>
    <tbody>
        @foreach (var grup in Model.Emteas.EmteaGroups)
        {
            groupCount = count;
            count = 0;

            @foreach (var emteaType in grup.EmteaTypes)
            {
                count++;
                dataInputCount++;
                var tuikCheck =
                emteaType.Tuiks.Sum(x => x.GuessValue);

                if (emteaType.DataInputs.Count() == 0)
                {
                    dataInputCount--;
                    <tr>
                        @if (count < 2)
                        {
                            <td class="tdstyle" rowspan="@grup.EmteaTypes.Count" height="395" align="left" valign="middle" bgcolor="#F0F0F0" sdnum="1033;1055;Standard"><b><font face="Verdana" color="#C00000">@grup.GroupName</font></b></td>
                        }

                        <td class="tdstyle">

                            <font face="Verdana" color="#C00000">
                                <b>
                                    @emteaType.EmteaTypeName
                                </b>
                                <br />
                                <span style="        text-transform: capitalize;
            font-style: italic">(@string.Join(',', emteaType.EmteaTypeGroups.Select(s => s.EmteaTypeGroupName)))</span>
        </font>

    </td>
    @for (int i = 0; i < 22; i++)
    {
        <td style="background-color:lightgray"><input type="number" readonly disabled /></td>
    }

</tr>
}
else
{
<tr class="@("datainput"+dataInputCount)" emteatype="@emteaType.Id" emteagroup="@grup.Id" dataInputId="@(DataInputModelCount>=dataInputCount ? Model.HaveTodayInputData==true ? Model.DataInputs[dataInputCount-1].Id:0:0)">

    @if (count < 2)
    {
        <td class="tdstyle" rowspan="@grup.EmteaTypes.Count" height="395" align="left" valign="middle" bgcolor="#F0F0F0" sdnum="1033;1055;Standard"><b><font face="Verdana" color="#C00000">@grup.GroupName</font></b></td>
    }

    <td class="tdstyle">

        <font face="Verdana" color="#C00000">
            <b>
                @emteaType.EmteaTypeName
            </b>
            <br />
            <span style="text-transform:capitalize;font-style:italic">(@string.Join(',', emteaType.EmteaTypeGroups.Select(s => s.EmteaTypeGroupName)))</span>
        </font>

    </td>
    @{ if (groupCount > count)

            count = ++groupCount;

    }
    <td>
        <input type="number" readonly name="tuik" value="@String.Format(new System.Globalization.CultureInfo("tr-TR").NumberFormat,"{0:###,###}",(@emteaType.DataInputs.Sum(x=>x.TuikValue)))" />
    </td>

    <td>
        <input type="number" readonly name="tmo" value="@String.Format(new System.Globalization.CultureInfo("tr-TR").NumberFormat,"{0:###,###}",(@emteaType.DataInputs.Sum(x=>x.GuessValue)))" />
    </td>

    <td><input type="number" readonly id="@("HasatYuzde_"+count.ToString())" name="percent" value="@String.Format(new System.Globalization.CultureInfo("tr-TR").NumberFormat,"{0:###,###}",(@emteaType.DataInputs.Sum(x => x.HasatOran * x.GuessValue) /@emteaType.DataInputs.Sum(x => x.GuessValue)))" /></td>
    <td><input type="number" readonly id="@("HasatEdilen_"+count.ToString())" name="hasatedilen" value="@String.Format(new System.Globalization.CultureInfo("tr-TR").NumberFormat,"{0:###,###}",(@emteaType.DataInputs.Sum(x=>x.HasatMiktar)))" /></td>
    <td><input type="number" readonly name="bekleyenton" value="@String.Format(new System.Globalization.CultureInfo("tr-TR").NumberFormat,"{0:###,###}",(@emteaType.DataInputs.Sum(x=>x.UreticiKalanMiktar)))" /></td>


