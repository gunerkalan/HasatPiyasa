    <td><input type="number" readonly name="toptan0" value="@String.Format(new System.Globalization.CultureInfo("tr-TR").NumberFormat,"{0:###,###}",(@emteaType.DataInputs.Sum(x=>x.ToptanPiyasa1*x.Natural1)/@emteaType.DataInputs.Sum(x=>x.Natural1)))" /></td>
    <td><input type="number" readonly name="toptan1" value="@String.Format(new System.Globalization.CultureInfo("tr-TR").NumberFormat,"{0:###,###}",(@emteaType.DataInputs.Sum(x=>x.ToptanPiyasa2*x.Natural2)/@emteaType.DataInputs.Sum(x=>x.Natural2)))" /></td>
    <td><input type="number" readonly name="toptan2" value="@String.Format(new System.Globalization.CultureInfo("tr-TR").NumberFormat,"{0:###,###}",(@emteaType.DataInputs.Sum(x=>x.ToptanPiyasa3*x.Natural3)/@emteaType.DataInputs.Sum(x=>x.Natural3)))" /></td>
